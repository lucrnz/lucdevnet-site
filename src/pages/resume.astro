---
import Head from "~/components/Head.astro";
import { siteCreationDate } from "~/consts";
import ResumeContent from "~/content/pages/resume/ResumeContent.astro";
import BaseLayout from "~/layouts/BaseLayout.astro";
const lastUpdated = new Date("2025-04-22T15:03:15.827Z");
---

<BaseLayout>
  <Head
    slot="head"
    title="Resume"
    metaDescription="My professional resume."
    datePublished={siteCreationDate}
    dateModified={lastUpdated}
  />
  <ResumeContent />
</BaseLayout>
