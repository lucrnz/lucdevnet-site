---
import BaseLayout from "~/layouts/BaseLayout.astro";
import Head from "~/components/Head.astro";
import { resumeData } from "~/data/resume/resumeData";
import ResumeBody from "~/components/resume/ResumeBody.astro";
import { siteCreationDate } from "~/consts";

const lastUpdated = new Date("2024-07-19T17:14:23Z");
---

<BaseLayout>
  <Head
    slot="head"
    title="Resume"
    metaDescription="My professional resume."
    datePublished={siteCreationDate}
    dateModified={lastUpdated}
  />
  <ResumeBody data={resumeData} />
</BaseLayout>
