---
import Head from "~/components/Head.astro";
import { siteTitle, siteDescription, siteCreationDate } from "~/consts";
import IndexPageContent from "~/content/pages/IndexPageContent.astro";
import BaseLayout from "~/layouts/BaseLayout.astro";
---

<BaseLayout>
  <Head
    slot="head"
    title={siteTitle}
    metaDescription={siteDescription}
    datePublished={siteCreationDate}
  />
  <IndexPageContent />
</BaseLayout>
